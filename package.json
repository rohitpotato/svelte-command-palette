{
	"name": "svelte-command-palette",
	"version": "2.0.0",
	"scripts": {
		"build:docs": "node generateDocSnippets.js",
		"dev": "npm run build:docs && vite dev",
		"build": "npm run build:docs && vite build",
		"package": "svelte-kit sync && svelte-package && cp ./README.md ./dist/README.md",
		"preview": "vite preview",
		"prepare": "svelte-kit sync",
		"test": "npm run test:unit && npm run test:e2e",
		"test:unit": "vitest run",
		"test:unit:watch": "vitest",
		"test:e2e": "playwright test",
		"test:e2e:ui": "playwright test --ui",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"release": "release-it",
		"pre:package-release": "rm -rf ./dist"
	},
	"devDependencies": {
		"@playwright/test": "^1.40.0",
		"@testing-library/svelte": "^5.0.0",
		"jsdom": "^24.0.0",
		"vitest": "^2.0.0",
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/package": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^4.0.0",
		"@typescript-eslint/eslint-plugin": "^6.0.0",
		"@typescript-eslint/parser": "^6.0.0",
		"autoprefixer": "^10.4.16",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.35.0",
		"postcss": "^8.4.32",
		"prettier": "^3.1.0",
		"prettier-plugin-svelte": "^3.1.0",
		"release-it": "^17.0.0",
		"shiki": "^1.0.0",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"tailwindcss": "^3.4.0",
		"tslib": "^2.6.0",
		"typescript": "^5.3.0",
		"vite": "^5.0.0"
	},
	"type": "module",
	"dependencies": {
		"csstype": "^3.1.3",
		"fuse.js": "^7.0.0",
		"tinykeys": "^2.1.0"
	},
	"peerDependencies": {
		"svelte": "^5.0.0"
	},
	"publishConfig": {
		"access": "public"
	},
	"keywords": [
		"svelte",
		"svelte-command-palette",
		"command-palette",
		"cmd-palette",
		"palette",
		"sveltejs",
		"sveltejs command palette",
		"svelte5"
	],
	"contributors": [
		{
			"email": "rohit.mmm1996@gmail.com",
			"name": "Rohit Kashyap",
			"url": "https://rohitpotato.vercel.app/"
		}
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/rohitpotato/svelte-command-palette.git"
	},
	"homepage": "https://svelte-command-palette.vercel.app/",
	"svelte": "./dist/index.js",
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js",
			"import": "./dist/index.js",
			"default": "./dist/index.js"
		}
	},
	"files": [
		"dist"
	],
	"license": "MIT"
}
